reset

set title "Histogram "
set xlabel "Sample"
set ylabel "Frequency"
set terminal png size 1920,1024 
set output "priority_rt+disturb.png"
max = 500000 #max value
min = 10000 #min value
width = (max-min)/10000

hist(x,width) = width*floor(x/width)+width/2.0

set xrange [min:max]
set yrange [0:100000]

set style fill solid 0.5 # fill style
set boxwidth width*0.9
plot "normal.txt" u (hist($1,width)):(1.0) smooth freq w boxes lc rgb"blue" notitle
